output "HDMI-A-1" {
	mode "1920x1080@240"
	scale 1
	transform "normal"
	position x=1600 y=0
	focus-at-startup
	backdrop-color "#181818"
	hot-corners {
		off
	}
}

output "eDP-1" {
	mode "1920x1080@144"
	scale 1.2
	transform "normal"
	position x=0 y=0
	focus-at-startup
	backdrop-color "#181818"
	hot-corners {
		off
	}
}
